{
    "name": "@w3nest/http-clients",
    "description": "HTTP clients for the w3nest ecosystem.",
    "version": "0.1.11",
    "author": "reinisch.gui@gmail.com",
    "license": "MIT",
    "homepage": "https://github.com/w3nest/http-clients#README.md",
    "main": "./dist/@w3nest/http-clients.js",
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "clean": "del-cli dist",
        "setup": "python .w3nest/setup.py",
        "build": "yarn build:prod",
        "pre-build": "yarn clean",
        "post-build": "del-cli dist/src/tests",
        "build:dev": "yarn pre-build && webpack --mode development && yarn post-build",
        "build:prod": "yarn pre-build && webpack --mode production && yarn post-build",
        "lint-check": "yarn lint-prettier-check && yarn lint-eslint-check",
        "lint-prettier-check": "prettier --check ./src",
        "lint-prettier-write": "prettier --write ./src",
        "lint-eslint-check": "eslint ./src/lib",
        "test": "jest -i",
        "test-coverage": "jest -i --collect-coverage",
        "doc": "typedoc",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "exports": {
        ".": {
            "default": "./dist/@w3nest/http-clients.js",
            "types": "./dist/src/index.d.ts",
            "webpm": {
                "dependencies": [
                    "rxjs"
                ],
                "aliases": []
            }
        }
    },
    "dependencies": {
        "rxjs": "^7.5.6"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^24.3.0",
        "@types/webpack": "^5.28.5",
        "adm-zip": "0.5.9",
        "del-cli": "^6.0.0",
        "eslint": "9.34.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "^29.0.1",
        "isomorphic-fetch": "^3.0.0",
        "jest": "^30.1.2",
        "jest-environment-jsdom": "^30.1.2",
        "jest-junit": "^16.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-properties": "^0.3.0",
        "ts-jest": "^29.4.1",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsx": "^4.20.5",
        "typedoc": "^0.28.12",
        "typescript": "5.9.2",
        "typescript-eslint": "8.41.0",
        "webpack": "^5.101.3",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "6.0.1"
    },
    "webpm": {
        "kind": "esm",
        "dependencies": {
            "rxjs": "^7.5.6"
        }
    },
    "webpack": {
        "root": "src",
        "apiVersion": "01",
        "main": "./index.ts",
        "additionalEntries": {},
        "externals": {
            "rxjs": [
                "rxjs_APIv7"
            ]
        }
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    }
}
